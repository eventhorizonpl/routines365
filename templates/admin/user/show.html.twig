                            </tr>
                            <tr>
                                <th scope="row">{{ 'Available email notifications'|trans }}</th>
                                <td>
                                    {% if user.account %}
                                        {{ user.account.availableEmailNotifications }}
                                    {% endif %}

                                    <a class="btn btn-info btn-sm float-right" href="{{ path('admin_account_operation_add_free_email', {'uuid': user.account}) }}" onclick="return confirm('Are you sure you want to execute this?');">
