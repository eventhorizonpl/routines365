{% extends 'admin_layout.html.twig' %}

{% block breadcrumb %}
    <li class="breadcrumb-item">
        <a href="{{ path('admin_workspace') }}">
            {{ 'Workspace'|trans }}
        </a>
    </li>
    <li class="breadcrumb-item">
        <a href="{{ path('admin_user_index') }}">
            {{ 'Users'|trans }}
        </a>
    </li>
    <li class="breadcrumb-item active">
        {{ 'User details'|trans }}
    </li>
{% endblock %}

{% block content %}
    <div class="card">
        <div class="card-header">
            {{ 'User details'|trans }}
            <a class="btn btn-info btn-sm float-right" href="{{ path('admin_user_index') }}">
                <i class="fas fa-arrow-left"></i>
                {{ 'Back'|trans }}
            </a>
            <ul class="nav nav-tabs card-header-tabs mt-2" id="myTab" role="tablist">
                <li class="nav-item" role="presentation">
                    <a class="nav-link active" id="basic-tab" data-toggle="tab" href="#basic" role="tab" aria-controls="home" aria-selected="true">{{ 'Basic'|trans }}</a>
                </li>
                <li class="nav-item" role="presentation">
                    <a class="nav-link" id="relations-tab" data-toggle="tab" href="#relations" role="tab" aria-controls="relations" aria-selected="true">{{ 'Relations'|trans }}</a>
                </li>
                <li class="nav-item" role="presentation">
                    <a class="nav-link" id="additional-account-tab" data-toggle="tab" href="#additional-account" role="tab" aria-controls="contact" aria-selected="false">{{ 'Additional account details'|trans }}</a>
                </li>
                <li class="nav-item" role="presentation">
                    <a class="nav-link" id="additional-profile-tab" data-toggle="tab" href="#additional-profile" role="tab" aria-controls="contact" aria-selected="false">{{ 'Additional profile details'|trans }}</a>
                </li>
                <li class="nav-item" role="presentation">
                    <a class="nav-link" id="additional-user-kyt-tab" data-toggle="tab" href="#additional-user-kyt" role="tab" aria-controls="contact" aria-selected="false">{{ 'Additional user KYT'|trans }}</a>
                </li>
                <li class="nav-item" role="presentation">
                    <a class="nav-link" id="additional-user-tab" data-toggle="tab" href="#additional-user" role="tab" aria-controls="contact" aria-selected="false">{{ 'Additional user details'|trans }}</a>
                </li>
            </ul>
        </div>
        <div class="card-body">
            <div class="tab-content" id="myTabContent">
                <div class="tab-pane fade show active" id="basic" role="tabpanel" aria-labelledby="basic-tab">
                    <table class="table">
                        <tbody>
                            <tr>
                                <th scope="row" style="width: 20%;">{{ 'Email'|trans }}</th>
                                <td>
                                    {{ user.email }}
                                    {% if user.isVerified %}
                                        <span class="badge badge-success">{{ 'Verified'|trans }}</span>
                                    {% else %}
                                        <span class="badge badge-danger">{{ 'Not verified'|trans }}</span>
                                    {% endif %}
                                </td>
                            </tr>
                            <tr>
                                <th scope="row">{{ 'First name'|trans }}</th>
                                <td>
                                    {% if user.profile %}
                                        {{ user.profile.firstName }}
                                    {% endif %}
                                </td>
                            </tr>
                            <tr>
                                <th scope="row">{{ 'Last name'|trans }}</th>
                                <td>
                                    {% if user.profile %}
                                        {{ user.profile.lastName }}
                                    {% endif %}
                                </td>
                            </tr>
                            <tr>
                                <th scope="row">{{ 'Country'|trans }}</th>
                                <td>
                                    {% if user.profile %}
                                        {{ user.profile.country }}
                                    {% endif %}
                                </td>
                            </tr>
                            <tr>
                                <th scope="row">{{ 'Phone'|trans }}</th>
                                <td>
                                    {% if user.profile and user.profile.phone %}
                                        {{ user.profile.phone|phone_number_format }}
