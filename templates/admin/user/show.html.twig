                                            <span class="badge badge-success">{{ 'Verified'|trans }}</span>
                                        {% else %}
                                            <span class="badge badge-danger">{{ 'Not verified'|trans }}</span>
                                        {% endif %}
                                    {% endif %}
                                </td>
                            </tr>
                            <tr>
                                <th scope="row">{{ 'Send weekly monthly statistics'|trans }}</th>
                                <td>
                                    {% if user.profile.sendWeeklyMonthlyStatistics %}
