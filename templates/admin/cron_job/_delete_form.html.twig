<form method="post" action="{{ path('admin_cron_job_delete', {'id': cron_job.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ cron_job.id) }}">
    <button class="btn btn-warning"><i class="fa fa-trash"></i> {{ 'Delete'|trans }}</button>
</form>
